{
  "name": "cap-app-template",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 8100",
    "patch": "patch-package",
    "android": "ionic cap sync && ionic cap copy android && ionic capacitor run android -l --external",
    "build": "yarn patch && vue-tsc && vite build",
    "preview": "vite preview --host 0.0.0.0 --port 8100",
    "lint": "eslint",
    "sync": "ionic cap sync",
    "build:android": "yarn patch && ionic cap copy android && ionic cap build android --no-open --prod --release",
    "pack:android:debug": "cd android && ./gradlew assembleDebug",
    "pacl:android:release": "cd android && ./gradlew assembleRelease"
  },
  "dependencies": {
    "@capacitor/android": "^5.7.0",
    "@capacitor/app": "5.0.7",
    "@capacitor/core": "5.7.0",
    "@capacitor/haptics": "5.0.7",
    "@capacitor/ios": "^5.7.0",
    "@capacitor/keyboard": "5.0.8",
    "@capacitor/status-bar": "^5.0.7",
    "@ionic/vue": "^7.0.0",
    "@ionic/vue-router": "^7.0.0",
    "capacitor-plugin-safe-area": "^2.0.6",
    "ionicons": "^7.0.0",
    "vue": "^3.3.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@capacitor/cli": "5.7.0",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.3.0",
    "cypress": "^13.5.0",
    "eslint": "^8.35.0",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^22.1.0",
    "sass": "^1.71.1",
    "terser": "^5.4.0",
    "typescript": "^5.1.6",
    "vite": "^5.0.0",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.0.24"
  },
  "description": "Mfuns Cap App Template"
}
